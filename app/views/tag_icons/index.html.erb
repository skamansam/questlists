<h1>Listing tag_icons</h1>

<table>
  <tr>
    <th>Tag</th>
    <th>Icon</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tag_icons.each do |tag_icon| %>
  <tr>
    <td><%= tag_icon.tag %></td>
    <td><%= tag_icon.icon %></td>
    <td><%= link_to 'Show', tag_icon %></td>
    <td><%= link_to 'Edit', edit_tag_icon_path(tag_icon) %></td>
    <td><%= link_to 'Destroy', tag_icon, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

